@model IEnumerable<QuanLySanPham.Models.Catalog>

@{
    ViewBag.Title = "Catalog List";
}

<h2>Catalog List</h2>

<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.CatalogCode)</th>
        <th>@Html.DisplayNameFor(model => model.CatalogName)</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.CatalogCode
            </td>
            <td>
                @item.CatalogName
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }
</table>